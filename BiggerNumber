using System;

namespace which_number_is_bigger
{
    class Program
    {
        static void Main(string[] args)
        {
            int Numberone, Numbertwo, Numberthree;
            Console.WriteLine("pls insert you're first number ");
            Numberone = Convert.ToInt16(Console.ReadLine());
            
            Console.WriteLine("pls insert you're second number ");
            Numbertwo = Convert.ToInt16(Console.ReadLine());
            
            Console.WriteLine("pls insert you're third number ");
            Numberthree = Convert.ToInt16(Console.ReadLine());
            

            Console.WriteLine("you're first number is : {0} \n you're second number is :{1} \n   you're third number is :{2} ", Numberone, Numbertwo, Numberthree);

            if (Numberone > Numbertwo && Numberone > Numberthree) 
                    {
                    Console.WriteLine("youre first number is bigger than other number");
                    }
            else if(Numbertwo > Numberone && Numbertwo > Numberthree)
                {
                    Console.WriteLine("youre second number is bigger than other number");
                }
            else if (Numberthree > Numberone && Numberthree >Numbertwo)
            {
                Console.WriteLine("youre third number bigger than other number");
            }
            Console.ReadKey(true);


        }
    }
}
